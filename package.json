    "@types/vue": "^2.0.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
